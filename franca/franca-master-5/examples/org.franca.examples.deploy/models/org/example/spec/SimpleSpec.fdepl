package org.example.spec

/**
 * This is a simple example specification for Franca's deployment model feature.
 */
specification SimpleSpec
{
	for strings {
		Encoding : {utf8, utf16, unicode};
	}

	for arrays {
		ArrayProp : Integer;
	}

	for maps {
		MaxElements : Integer;
	}
	
	for map_keys {
		KeyProp : Boolean;
	}

	for map_values {
		ValProp : Boolean;
	}
}
